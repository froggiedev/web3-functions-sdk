{
  "name": "@gelatonetwork/js-resolver-sdk",
  "version": "0.1.0",
  "description": "Gelato Automate JS resolvers sdk",
  "url": "https://github.com/gelatodigital/js-resolver-poc",
  "main": "dist/lib/index.js",
  "types": "dist/lib/types/index.d.ts",
  "files": [
    "dist/lib/**/*"
  ],
  "exports": {
    ".": "./dist/lib/index.js",
    "./net": "./dist/lib/net/index.js",
    "./runtime": "./dist/lib/runtime/index.js",
    "./provider": "./dist/lib/provider/index.js",
    "./builder": "./dist/lib/builder/index.js",
    "./uploader": "./dist/lib/uploader/index.js"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/lib/index.d.ts"
      ],
      "net": [
        "dist/lib/net/index.d.ts"
      ],
      "provider": [
        "dist/lib/provider/index.d.ts"
      ],
      "runtime": [
        "dist/lib/runtime/index.d.ts"
      ],
      "builder": [
        "dist/lib/builder/index.d.ts"
      ],
      "uploader": [
        "dist/lib/uploader/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json && yarn deps",
    "deps": "yarn link && yarn link @gelatonetwork/js-resolver-sdk",
    "format": "prettier --write '*/**/*.{js,json,md,ts}'",
    "format:check": "prettier --check '*/**/*.{js,json,md,ts}'",
    "lint": "eslint --cache .",
    "test": "ts-node src/scripts/test.ts",
    "benchmark": "ts-node src/scripts/benchmark.ts",
    "upload": "ts-node src/scripts/upload.ts",
    "fetch": "ts-node src/scripts/fetch.ts",
    "schema": "ts-node src/scripts/schema.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.0"
  },
  "dependencies": {
    "@types/dockerode": "^3.3.11",
    "ajv": "^8.11.0",
    "axios": "1.1.2",
    "body-parser": "^1.20.1",
    "colors": "^1.4.0",
    "deno-bin": "^1.28.1",
    "dockerode": "^3.3.4",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.11",
    "eth-rpc-errors": "^4.0.3",
    "ethers": "^5.7.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "ky": "^0.32.2",
    "pidusage": "^3.0.1",
    "tar": "^6.1.12"
  }
}
